{
	"schemaVersion": 1,
	"id": "osl-branding",
	"version": "${version}",
	"name": "Branding Patch",
	"description": "A mod that patches the title screen with mod loader information.",
	"authors": [
		"OrnitheMC",
		"Space Walker"
	],
	"contact": {
		"homepage": "https://ornithemc.net/",
		"issues": "https://github.com/OrnitheMC/ornithe-standard-libraries/issues",
		"sources": "https://github.com/OrnitheMC/ornithe-standard-libraries"
	},
	"license": "Apache-2.0",
	"icon": "assets/ornithe-standard-libraries/branding/icon.png",
	"environment": "${environment}",
	"entrypoints": {
		"client-init": [
			"net.ornithemc.osl.branding.impl.BrandingPatchImpl"
		]
	},
	"mixins": [
		"osl.branding.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.0",
		"minecraft": "${mc_version_range}",
		"osl-core": ">=0.4.0",
		"osl-entrypoints": ">=0.4.0",
		"osl-lifecycle-events": ">=0.5.0"
	}
}
